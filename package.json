{
	"name": "explorer-format",
	"version": "1.0.0",
	"license": "MIT",
	"scripts": {
		"watch": "parcel watch",
		"build": "parcel build",
		"build-format": "parcel build && ./bin/build-format",
		"build-basic": "yarn build-format && tweego -o tests/test-stories/output/index.html -l --log-files tests/test-stories/basic.twee"
	},
	"devDependencies": {
		"parcel": "^2.7.0"
	},
	"targets": {
		"default": {
			"source": "src/index.ts",
			"distDir": "build",
			"sourceMap": {
				"inline": true,
				"inlineSources": true
			}
		}
	},
	"dependencies": {
		"handlebars": "^4.7.7",
		"marked": "^4.1.0"
	}
}
